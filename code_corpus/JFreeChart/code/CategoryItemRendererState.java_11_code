    public int getVisibleSeriesIndex(int rowIndex) {
    	if (this.visibleSeries == null) {
    	    return rowIndex;
    	}
		int index = -1;
		for (int vRow = 0; vRow < this.visibleSeries.length ; vRow++){
			if (this.visibleSeries[vRow] == rowIndex) {
				index = vRow;
				break;
			}
		}
		return index;
    }

