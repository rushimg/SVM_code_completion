    public boolean equals(Object obj) {

        if (obj == null) {
            return false;
        }

        if (obj == this) {
            return true;
        }

        if (!(obj instanceof HistogramType)) {
            return false;
        }

        HistogramType t = (HistogramType) obj;
        if (!this.name.equals(t.name)) {
            return false;
        }

        return true;

    }

