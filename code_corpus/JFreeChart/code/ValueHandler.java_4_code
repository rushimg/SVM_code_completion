    public void startElement(String namespaceURI,
                             String localName,
                             String qName,
                             Attributes atts) throws SAXException {

        if (qName.equals(VALUE_TAG)) {
            // no attributes to read
            clearCurrentText();
        }
        else {
            throw new SAXException("Expecting <Value> but found " + qName);
        }

    }

