  public static <T> ObjectStream<T> createObjectStream(final T... array) {

    return new ObjectStream<T>() {

      private int index = 0;

      public T read() {
        if (index < array.length)
          return array[index++];
        else
          return null;
      }

      public void reset() {
        index = 0;
      }

      public void close() {
      }
    };
  }

