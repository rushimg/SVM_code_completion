  @SuppressWarnings("unchecked")
  public ArrayCoreMap(CoreMap other) {
    Set<Class<?>> otherKeys = other.keySet();

    size = otherKeys.size();
    keys = new Class[size];
    values = new Object[size];

    int i = 0;
    for (Class key : otherKeys) {
      this.keys[i] = key;
      this.values[i] = other.get(key);
      i++;
    }
  }

