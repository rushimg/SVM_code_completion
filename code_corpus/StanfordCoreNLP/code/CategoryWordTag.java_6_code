  @Override
  public String toString() {
    if (category() != null) {
      if ((word() == null || tag() == null) || !printWordTag || (suppressTerminalDetails && (word().equals(category()) || tag().equals(category())))) {
        return category();
      } else {
        return category() + "[" + word() + "/" + tag() + "]";
      }
    } else {
      if (tag() == null) {
        return word();
      } else {
        return word() + "/" + tag();
      }
    }
  }


