  public static<E> Set<E> retainKeys(Counter<E> counter, Collection<E> matchKeys) {
    Set<E> removed = Generics.newHashSet();
    for (E key : counter.keySet()) {
      boolean matched = matchKeys.contains(key);
      if (!matched) {
        removed.add(key);
      }
    }
    for (E key : removed) {
      counter.remove(key);
    }
    return removed;
  }

