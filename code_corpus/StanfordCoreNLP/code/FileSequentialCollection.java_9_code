  public FileSequentialCollection(Collection<?> c, FileFilter filt, boolean includeDirs) {
    super();
    // store the arguments.  They are expanded by the iterator
    if (c == null) {
      coll = new ArrayList<Object>();
    } else {
      coll = c;
    }
    this.filt = filt;
    this.includeDirs = includeDirs;
  }


