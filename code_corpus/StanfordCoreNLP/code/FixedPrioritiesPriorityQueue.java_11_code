  public E removeFirst() throws NoSuchElementException {
    E first = getFirst();
    swap(0, size - 1);
    size--;
    elements.remove(size);
    heapifyDown(0);
    return first;
  }

  public List<E> toSortedList() {
    // initialize with size
    List<E> list = new ArrayList<E>();
    while (hasNext()) {
      list.add(next());
    }
    return list;
  }


