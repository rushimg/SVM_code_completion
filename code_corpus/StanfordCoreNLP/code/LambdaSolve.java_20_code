
  public double GainCompute(Feature f, double errorGain) {
    double r = (f.ftilde() > fExpected(f) ? 1.0 : -1.0);
    f.initHashVals();
    int iterations = 0;
    double alfa = 0.0;
    GSF(alfa, f);
    double gsfValNew = 0.0;
    while (iterations < 30) {
      iterations++;
      gsfValNew = GSF(alfanext, f);
      if (Math.abs(alfanext - alfa) < errorGain) {
        return gsfValNew;
      }
      alfa = alfanext;
    }
    return gsfValNew;
  }


