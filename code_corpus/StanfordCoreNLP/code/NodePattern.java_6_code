  public static class NegateNodePattern<T> extends NodePattern<T> {
    NodePattern<T> p;

    public NegateNodePattern(NodePattern<T> p) {
      this.p = p;
    }

    public boolean match(T node)
    {
      return !p.match(node);
    }

    public String toString() {
      return "!" + p;
    }
  }

