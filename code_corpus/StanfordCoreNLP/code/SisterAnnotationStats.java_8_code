  public static List<String> leftSisterLabels(Tree t, Tree p) {
    List<String> l = new ArrayList<String>();
    if (p == null) {
      return l;
    }
    Tree[] kids = p.children();
    for (int i = 0; i < kids.length; i++) {
      if (kids[i].equals(t)) {
        break;
      } else {
        l.add(0, kids[i].label().value());
      }
    }
    return l;
  }

