   public boolean isLabelAnnotationIntroducingCharacter(char ch) {
     if (tlp.isLabelAnnotationIntroducingCharacter(ch)) {
       return true;
     }
     //for heads, there's one more char we want to check because we don't care about grammatical fns
     if (ch == '-') {
       return true;
     }
     return false;
   }


