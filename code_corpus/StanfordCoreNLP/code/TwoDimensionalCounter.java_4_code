  public int sizeOuterMap(){
    return map.size();
  }

  public boolean containsKey(K1 o1, K2 o2) {
    if (!map.containsKey(o1))
      return false;
    ClassicCounter<K2> c = map.get(o1);
    return c.containsKey(o2);
  }
  
  public boolean containsFirstKey(K1 o1) {
    if (!map.containsKey(o1))
      return false;
    else
      return true;
  }

