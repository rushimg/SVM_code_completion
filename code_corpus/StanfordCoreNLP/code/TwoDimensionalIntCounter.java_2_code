  public IntCounter<K2> getCounter(K1 o) {
    IntCounter<K2> c = map.get(o);
    if (c == null) {
      c = new IntCounter<K2>(innerMF);
      c.setDefaultReturnValue(defaultValue);
      map.put(o, c);
    }
    return c;
  }

  public Set<Map.Entry<K1,IntCounter<K2>>> entrySet(){
    return map.entrySet();
  }

