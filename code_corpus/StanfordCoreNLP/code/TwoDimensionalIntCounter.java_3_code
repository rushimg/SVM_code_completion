  public int size() {
    int result = 0;
    for (K1 o : firstKeySet()) {
      IntCounter<K2> c = map.get(o);
      result += c.size();
    }
    return result;
  }

  public boolean containsKey(K1 o1, K2 o2) {
    if (!map.containsKey(o1)) return false;
    IntCounter<K2> c = map.get(o1);
    return c.containsKey(o2);
  }

