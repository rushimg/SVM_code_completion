  public int compareTo(WordLemmaTag wordLemmaTag) {
    int first = word().compareTo(wordLemmaTag.word());
    if (first != 0)
      return first;
    int second = lemma().compareTo(wordLemmaTag.lemma());
    if (second != 0)
      return second;
    else
      return tag().compareTo(wordLemmaTag.tag());
  }


