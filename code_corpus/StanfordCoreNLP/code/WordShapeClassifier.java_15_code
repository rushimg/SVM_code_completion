  private static String wordShapeDigits(final String s) {
    char[] outChars = null;

    for (int i = 0; i < s.length(); i++) {
      char c = s.charAt(i);
      if (Character.isDigit(c)) {
        if (outChars == null) {
          outChars = s.toCharArray();
        }
        outChars[i] = '9';
      }
    }
    if (outChars == null) {
      // no digit found
      return s;
    } else {
      return new String(outChars);
    }
  }


