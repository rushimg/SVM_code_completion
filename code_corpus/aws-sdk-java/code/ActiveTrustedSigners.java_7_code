    public void setSigners(java.util.Collection<Signer> signers) {
        if (signers == null) {
            this.signers = null;
            return;
        }

        java.util.List<Signer> signersCopy = new java.util.ArrayList<Signer>(signers.size());
        signersCopy.addAll(signers);
        this.signers = signersCopy;
    }
    
