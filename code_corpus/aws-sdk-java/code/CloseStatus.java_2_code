public enum CloseStatus {
    
    COMPLETED("COMPLETED"),
    FAILED("FAILED"),
    CANCELED("CANCELED"),
    TERMINATED("TERMINATED"),
    CONTINUED_AS_NEW("CONTINUED_AS_NEW"),
    TIMED_OUT("TIMED_OUT");

    private String value;

    private CloseStatus(String value) {
        this.value = value;
    }

    @Override
    public String toString() {
        return this.value;
    }

