    public void setApps(java.util.Collection<App> apps) {
        if (apps == null) {
            this.apps = null;
            return;
        }
        com.amazonaws.internal.ListWithAutoConstructFlag<App> appsCopy = new com.amazonaws.internal.ListWithAutoConstructFlag<App>(apps.size());
        appsCopy.addAll(apps);
        this.apps = appsCopy;
    }
    
