public enum EnvironmentStatus {
    
    Launching("Launching"),
    Updating("Updating"),
    Ready("Ready"),
    Terminating("Terminating"),
    Terminated("Terminated");

    private String value;

    private EnvironmentStatus(String value) {
        this.value = value;
    }

    @Override
    public String toString() {
        return this.value;
    }

