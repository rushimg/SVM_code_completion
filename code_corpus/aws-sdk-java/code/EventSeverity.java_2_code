public enum EventSeverity {
    
    TRACE("TRACE"),
    DEBUG("DEBUG"),
    INFO("INFO"),
    WARN("WARN"),
    ERROR("ERROR"),
    FATAL("FATAL");

    private String value;

    private EventSeverity(String value) {
        this.value = value;
    }

    @Override
    public String toString() {
        return this.value;
    }

