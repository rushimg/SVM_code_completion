public enum InstanceStateName {
    
    Pending("pending"),
    Running("running"),
    ShuttingDown("shutting-down"),
    Terminated("terminated"),
    Stopping("stopping"),
    Stopped("stopped");

    private String value;

    private InstanceStateName(String value) {
        this.value = value;
    }

    @Override
    public String toString() {
        return this.value;
    }

