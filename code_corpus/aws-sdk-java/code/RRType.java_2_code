public enum RRType {
    
    SOA("SOA"),
    A("A"),
    TXT("TXT"),
    NS("NS"),
    CNAME("CNAME"),
    MX("MX"),
    PTR("PTR"),
    SRV("SRV"),
    SPF("SPF"),
    AAAA("AAAA");

    private String value;

    private RRType(String value) {
        this.value = value;
    }

    @Override
    public String toString() {
        return this.value;
    }

