public enum StepExecutionState {
    
    PENDING("PENDING"),
    RUNNING("RUNNING"),
    CONTINUE("CONTINUE"),
    COMPLETED("COMPLETED"),
    CANCELLED("CANCELLED"),
    FAILED("FAILED"),
    INTERRUPTED("INTERRUPTED");

    private String value;

    private StepExecutionState(String value) {
        this.value = value;
    }

    @Override
    public String toString() {
        return this.value;
    }

