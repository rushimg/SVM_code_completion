public enum VerificationStatus {
    
    Pending("Pending"),
    Success("Success"),
    Failed("Failed"),
    TemporaryFailure("TemporaryFailure"),
    NotStarted("NotStarted");

    private String value;

    private VerificationStatus(String value) {
        this.value = value;
    }

    @Override
    public String toString() {
        return this.value;
    }

