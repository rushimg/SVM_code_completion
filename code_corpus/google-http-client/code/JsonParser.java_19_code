  @Beta
  public Object parse(Type dataType, boolean close, CustomizeJsonParser customizeParser)
      throws IOException {
    try {
      if (!Void.class.equals(dataType)) {
        startParsing();
      }
      return parseValue(null, dataType, new ArrayList<Type>(), null, customizeParser, true);
    } finally {
      if (close) {
        close();
      }
    }
  }

