public class VertexPair<V>
{
    //~ Instance fields --------------------------------------------------------

    private V n1;
    private V n2;

    //~ Constructors -----------------------------------------------------------

    public VertexPair(V n1, V n2)
    {
        this.n1 = n1;
        this.n2 = n2;
    }

    //~ Methods ----------------------------------------------------------------

    public V getFirst()
    {
        return n1;
    }

    public V getSecond()
    {
        return n2;
    }

