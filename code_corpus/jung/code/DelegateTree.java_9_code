	@Override
	public boolean removeVertex(V vertex) {
	    if (!delegate.containsVertex(vertex))
	        return false;
		for(V v : getChildren(vertex)) {
			removeVertex(v);
            vertex_depths.remove(v);
		}
        
        // recalculate height
		vertex_depths.remove(vertex);
		return delegate.removeVertex(vertex);
	}
	
