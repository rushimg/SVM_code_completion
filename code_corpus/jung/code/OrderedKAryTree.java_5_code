    public int getChildCount(V vertex) {
        if (!containsVertex(vertex)) 
            return 0;
        List<E> edges = vertex_data.get(vertex).child_edges;
        if (edges == null)
        	return 0;
        int count = 0;
        for (E edge : edges)
            count += edge == null ? 0 : 1;
    
        return count;
    }
  
