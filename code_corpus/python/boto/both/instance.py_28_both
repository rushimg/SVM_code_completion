    def stop(self, force=False, dry_run=False):
        """
        Stop the instance
        :type force: bool
        :param force: Forces the instance to stop
        :rtype: list
        :return: A list of the instances stopped
        """
        rs = self.connection.stop_instances([self.id], force, dry_run=dry_run)
        if len(rs) > 0:
            self._update(rs[0])
