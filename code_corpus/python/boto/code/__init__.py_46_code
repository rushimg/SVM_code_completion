    def associate_dhcp_options(self, dhcp_options_id, vpc_id, dry_run=False):
        
        params = {'DhcpOptionsId': dhcp_options_id,
                  'VpcId': vpc_id}
        if dry_run:
            params['DryRun'] = 'true'
        return self.get_status('AssociateDhcpOptions', params)
