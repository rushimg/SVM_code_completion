    def enable_vgw_route_propagation(self, route_table_id, gateway_id,
                                     dry_run=False):
        
        params = {
            'RouteTableId': route_table_id,
            'GatewayId': gateway_id,
        }
        if dry_run:
            params['DryRun'] = 'true'
        return self.get_status('EnableVgwRoutePropagation', params)
