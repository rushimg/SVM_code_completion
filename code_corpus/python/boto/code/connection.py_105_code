    def detach_network_interface(self, attachment_id, force=False,
                                 dry_run=False):
        
        params = {'AttachmentId': attachment_id}
        if force:
            params['Force'] = 'true'
        if dry_run:
            params['DryRun'] = 'true'
        return self.get_status('DetachNetworkInterface', params, verb='POST')
