    def monitor_instances(self, instance_ids, dry_run=False):
        
        params = {}
        self.build_list_params(params, instance_ids, 'InstanceId')
        if dry_run:
            params['DryRun'] = 'true'
        return self.get_list('MonitorInstances', params,
                             [('item', InstanceInfo)], verb='POST')
