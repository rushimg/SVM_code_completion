    def get_password_data(self, instance_id, dry_run=False):
        
        params = {'InstanceId': instance_id}
        if dry_run:
            params['DryRun'] = 'true'
        rs = self.get_object('GetPasswordData', params, ResultSet, verb='POST')
        return rs.passwordData
