    def create_volume(self, zone, size=None, volume_type=None, iops=None,
                      dry_run=False):
        
        if isinstance(zone, Zone):
            zone = zone.name
        return self.connection.create_volume(
            size,
            zone,
            self.id,
            volume_type,
            iops,
            self.encrypted,
            dry_run=dry_run
        )
