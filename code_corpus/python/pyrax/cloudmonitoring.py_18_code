    def list(self, entity=None):
        """
        Returns a dictionary of changelog data, optionally filtered for a given
        entity.
        """
        uri = "/%s" % self.uri_base
        if entity:
            uri = "%s?entityId=%s" % (uri, utils.get_id(entity))
        resp, resp_body = self._list(uri, return_raw=True)
        return resp_body



