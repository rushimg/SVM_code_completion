public boolean isUseful(char inputChar) {
        if ((inputChar >= '0') && (inputChar <= '9')) {
            return true;       //is number
        }
        if ((inputChar >= 'a') && (inputChar <= 'z')) {
            return true;       //is lower letter
        }
        if ((inputChar >= 'A') && (inputChar <= 'Z')) {
            return true;       //is capital letter
        }
        
        return false;
    }
